{
    "manifest_version": 2,
    "name": "IoT Demo",
    "description": "Show IoT Demo",
    "version": "0.0.1",
	"background": {
        "scripts": ["js/background.js"]
	},
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_title": "IoT Demo Plugin"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://code.jquery.com https://api.map.baidu.com https://cdnjs.cloudflare.com https://maxcdn.bootstrapcdn.com; object-src 'self'",
    "permissions": [       
        "tabs",            
        "webRequestBlocking",
        "webRequest",
        "browsingData",
        "storage",
        "unlimitedStorage"
    ],
    "icons" : {
        "128": "images/icon.png"
    },
    "options_page":"index.html"
}
